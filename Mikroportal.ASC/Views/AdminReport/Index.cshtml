<script>
    var filesToUpload = [];

    $.fn.fileUploader = function (filesToUpload, sectionIdentifier) {
        var fileIdCounter = 0;

        this.closest(".files").change(function (evt) {
            var output = [];

            for (var i = 0; i < evt.target.files.length; i++) {
                fileIdCounter++;

                var file = evt.target.files[i];
                var fileId = sectionIdentifier + fileIdCounter;

                filesToUpload.push({
                    id: fileId,
                    file: file
                });

                var removeLink = "<div class=\"removeFile btn btn-danger btn-xs\" data-fileid=\"" + fileId + "\"><i class='fa fa-times-circle-o' aria-hidden='true'></i></div>";

                var dataContent = "";

                if (file.type == "image/jpeg" || file.type == "image/png") {
                    dataContent += "<li class='file-box'>" +
                        //removeLink +
                        "<div class='file'>" +
                        "<a href='#'>" +
                        "<span class='corner'></span>" +

                        "<div class='icon'>" +
                        "<img alt='image' class='feed-photo' src=" + (window.URL ? URL : webkitURL).createObjectURL(file) + ">" +
                        "</div>" +
                        "<div class='file-name'>" +
                        escape(file.name) +
                        "<br>" +

                        "</div>" +
                        "</a>" +
                        "</div>" +
                        "</li>";
                } else {
                    dataContent += "<li class='file-box'>" +
                        //removeLink +
                        "<div class='file'>" +
                        "<a href='#'>" +
                        "<span class='corner'></span>" +

                        "<div class='icon'>" +
                        "<i class='fa fa-file'></i>" +
                        "</div>" +
                        "<div class='file-name'>" +
                        escape(file.name) +
                        "<br>" +

                        "</div>" +
                        "</a>" +
                        "</div>" +
                        "</li>";
                }


                $(".attachment").append(dataContent);

            };
            //evt.target.value = null;
        });

        this.clear = function () {
            for (var i = 0; i < filesToUpload.length; ++i) {
                if (filesToUpload[i].id.indexOf(sectionIdentifier) >= 0)
                    filesToUpload.splice(i, 1);
            }

            $(this).children(".fileList").empty();
        }

        return this;
    };

    (function () {
        var files1Uploader = $("#files").fileUploader(filesToUpload, "files");

    })()


    $('body').on('click', '.removeFile', function (e) {
        var fileId = $(this).data("fileid");
        // loop through the files array and check if the name of that file matches FileName
        // and get the index of the match
        for (var i = 0; i < filesToUpload.length; ++i) {
            if (filesToUpload[i].id === fileId)
                filesToUpload.splice(i, 1);
        }
        $(this).parent().remove();

        //$("#files").fileUploader(filesToUpload)
        $("#files").attr("value", filesToUpload);
    });




    $(document).ready(function () {
        getInstallationReportList();
    });
    $("#searchSerialNo").bind("keyup change keypress", function (e) {
        getInstallationReportList();
    });
    $("#searchSkuCode").bind("keyup change keypress", function (e) {
        getInstallationReportList();
    });
    $("#searchTracingName").bind("keyup change keypress", function (e) {
        getInstallationReportList();
    });

    function getInstallationReportList() {
        $('#reportListDataContent').text("");
        $.ajax({
            type: "POST",
            dataType: 'json',
            data: { searchSerialNo: $("#searchSerialNo").val(), searchSkuCode: $("#searchSkuCode").val(), searchTracingName: $("#searchTracingName").val(), searchStartDate: $("#searchStartDate").val(), searchEndDate: $("#searchEndDate").val(), searchStatus: $("#searchStatus").val(), searchServicesType: $("#searchServicesType").val() },
            url: "@ViewData["UrlPath"]/api/AdminReport/GetMachineHistoryList",
            success: function (data) {

                $('#reportListDataContent').text("");
                $(".unreadFlags").text(data.readFlagsNothingCount);
                var tr;
                $(data.getInstallationList).each(function (index, element) {
                    tr = "<tr row='" + element.machineryServiceId + "' class='read'>";
                    if (element.serviceName == "Kurulum") {
                        tr += "<td> <span class='label label-primary '>" + element.serviceName + "</span></td>";
                    } else if (element.serviceName == "Yenileme") {
                        tr += "<td> <span class='label label-warning '>" + element.serviceName + "</span></td>";
                    } else if (element.serviceName == "Tamir ve Bakım") {
                        tr += "<td> <span class='label label-info '>" + element.serviceName + "</span></td>";
                    } else if (element.serviceName == "Çağrı Merkezi") {
                        tr += "<td> <span class='label label-success'>" + element.serviceName + "</span></td>";
                    } else if (element.serviceName == "Garanti İptali") {
                        tr += "<td> <span class='label label-danger'>" + element.serviceName + "</span></td>";
                    } else {
                        tr += "<td> <span class='label label-default'>" + element.serviceName + "</span></td>";
                    }
                    tr += "<td class='mail-date'>" + element.createdDateFormat + "</td>";

                    if (element.userName == null || element.userName == undefined) {
                        tr += "<td></td>";
                    } else {

                        tr += "<td>" + element.userName + "</td>";
                    }

                    if (element.serviceDate == null || element.serviceDate == undefined) {
                        tr += "<td></td>";
                    } else {

                        tr += "<td>" + element.serviceDate + "</td>";
                    }

                    if (element.serialNo == null || element.serialNo == undefined) {
                        tr += "<td></td>";
                    } else {
                        tr += "<td>" + element.serialNo + "</td>";

                    }
                    if (element.tracingName == null || element.tracingName == undefined) {
                        tr += "<td></td>";
                    } else {
                        tr += "<td>" + element.tracingName + "</td>";

                    }

                    if (element.skuCode == null || element.skuCode == undefined) {
                        tr += "<td></td>";
                    } else {
                        tr += "<td>" + element.skuCode + "</td>";

                    }

                    if (element.companyName == null || element.stafName == undefined) {
                        element.companyName = '';
                    }
                    tr += "<td>" + element.companyName + "</td>";
                    if (element.serviceName != "Uzatmalı Garanti" && element.serviceName != "Garanti İptali") {

                        if (element.serviceStatus == "Beklemede") {
                            tr += "<td> <span class='label label-warning'>Beklemede</span></td>";
                        } else if (element.serviceStatus == "Onaylandı") {
                            tr += " <td><span class='label label-info '>Onaylandı</span></td>";
                        } else if (element.serviceStatus == "Reddedildi") {
                            tr += "<td> <span class='label label-warring '>Reddedildi</span></td>";
                        } else {
                            tr += "<td></td>"
                        }
                    } else {
                        tr += "<td></td>"
                    }

                    tr += "<td>" +
                        "<button onclick='inboxDetail(" + element.machineryServiceId + ")' type='button' class='btn   btn-sm btn-default' data-toggle='modal' data-target='#inboxDetailInstallationModal'>Detay</button>" +
                        "<button onclick='fileUploadModal(" + element.machineryServiceId + ")' type='button' class='btn  btn-sm btn-default' data-toggle='modal' data-target='#inboxInstallationFileModal'>Dokuman Ekle</button>" +
                        "</td>";
                    tr += "</tr>";
                    $('#reportListDataContent').append(tr);
                });
            },
            error: function (e) {


            },
            cache: false
        });

    }

